<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_mrp_production_inherit_evr_fields" model="ir.ui.view">
        <field name="name">mrp.production.evr.fields.view</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>

        <field name="arch" type="xml">
            <!-- Insert inside Miscellaneous (Other Info) page -->
            <xpath expr="//field[@name='project_id']" position="after">

                    <field name="root_bom_id"/>
                    <field name="parent_mo_id"/>
                    <field name="can_manufacture"/>
                    <field name="line"/>
                <field name="branch_intermediate_location_id"/>
                <field name="cr_final_location_id"/>

            </xpath>
             <xpath expr="//field[@name='user_id']" position="after">
                 <field name="branch_mapping_id"/>
             </xpath>
        </field>
    </record>
</odoo>
